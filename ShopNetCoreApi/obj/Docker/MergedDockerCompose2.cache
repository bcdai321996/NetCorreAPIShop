services:
  db:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Bcdai@321996
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      your-network: {}
    ports:
    - published: 1433
