services:
  db:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Bcdai@321996
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      your-network: null
    ports:
    - mode: ingress
      target: 1433
      published: 1433
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      target: /var/lib/sql/data
      volume: {}
  shopnetcoreapi:
    build:
      context: C:\Users\buico\Desktop\CodeManagerment\NetCorreAPIShop\ShopNetCoreApi
      dockerfile: ShopNetCoreApi/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ShopNetCoreApi
      target: base
    container_name: ShopNetCoreApi
    depends_on:
      db:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: shopnetcoreapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/ShopNetCoreApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      your-network: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 80
      published: 9080
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Users\buico\Desktop\CodeManagerment\NetCorreAPIShop\ShopNetCoreApi\ShopNetCoreApi
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\buico\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\buico\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
